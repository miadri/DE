/* this program ejecut methody search lineal
*/
#include<iostream>
#include<conio.h>
using namespace 	std;
int main (){
	int A[]= {2,4,6,7,9};
	int dato = 6;
	int posicion = 0;
	
	
	for(int i=0; i <5; i++){
		if (A [i] == dato){
			posicion = A [i];
	
			}
		break;
		}
		cout<< "el numero fue encontrado en la posicion : ", A[i];
		getch ();
	
	}
